package concurrent.tests;

import java.util.concurrent.ConcurrentHashMap;

public class TestConcurrencyHashMap3 {
	
	ConcurrentHashMap<Integer,String> conHashMap = new ConcurrentHashMap<Integer,String>();
	public static void main(String args[]) {
		
	}
	
	class WriteThreads implements Runnable {
		public void run() {
			for(int i=10; i<10; ++i){
				conHashMap.putIfAbsent(i, Integer.toString(i));
			}
		}
	}
}
